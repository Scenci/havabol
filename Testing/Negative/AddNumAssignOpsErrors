print("ADD INTEGERS TO STRINGS");
String x = "inty";
print(x, "\t--Add 5, oops can't");
x += 5;
print(x, "\t--Subtract 10");
x -= 10;
print(x, "\t--Subtract 5");
x -= 5;
print(x, "\t--Final Answer");

print("");
print("ADD FLOATS TO STRINGS");
String y = "floaty";
print(y, "\t--Add 3.14, oops can't");
y += 3.14;
print(y, "\t--Add 28.28");
y += 28.28;
print(y, "\t--Subtract 31.42");
y -= 31.42;
print(y, "\t--Substract 6.28");
y -= 6.28;
print(y, "\t--Final Answer");